{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "A NodeJS starter repo for working with Express & Typescript",
  "main": "src/index.js",
  "repository": "git@github.com:jimboslicethat/express-typescript-starter.git",
  "author": "James Walsh <jamesandersonwalsh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-winston": "^3.0.4",
    "@types/winston": "^2.4.4",
    "nodemon": "^1.19.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-winston": "^3.3.0",
    "winston": "^3.2.1"
  }
}
